#include<stdio.h>
int main()
{
    int a,b,r,c, sum=0;
    
    int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    a=b=3;
 for(r=0;r<a;r++)
    {
        for(c=0;c<b;c++)
        {
          if(r<c)
          {
          sum = sum + arr[r][c];
            }
        }
    }
printf("%d",sum);

    return  0;
}
